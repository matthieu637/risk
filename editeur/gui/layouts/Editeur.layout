<?xml version="1.0" encoding="UTF-8"?>

<GUILayout >
    <Window Type="DefaultWindow" Name="Editeur" >
	<Property Name="MousePassThroughEnabled" Value="True" />
	<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
        <LayoutImport Filename="Menu.layout" />
        <Window Type="TaharezLook/VerticalScrollbar" Name="Editeur/VSB" >
            <Property Name="PageSize" Value="0" />
            <Property Name="StepSize" Value="1" />
            <Property Name="OverlapSize" Value="0" />
            <Property Name="DocumentSize" Value="1" />
            <Property Name="ScrollPosition" Value="0" />
            <Property Name="UnifiedAreaRect" Value="{{1,-13},{0.03,0},{1,0},{1,0}}" />
	    <Event Name="ScrollPosChanged" Function="gui_viewscroll_change_vertical" />
        </Window>
        <Window Type="TaharezLook/HorizontalScrollbar" Name="Editeur/HSB" >
            <Property Name="PageSize" Value="0" />
            <Property Name="StepSize" Value="1" />
            <Property Name="OverlapSize" Value="0" />
            <Property Name="DocumentSize" Value="1" />
            <Property Name="ScrollPosition" Value="0" />
            <Property Name="UnifiedAreaRect" Value="{{0,0},{0.985,0},{1,-10},{1,0}}" />
            <Event Name="ScrollPosChanged" Function="gui_viewscroll_change_horizontal" />
        </Window>
    </Window>
</GUILayout>
